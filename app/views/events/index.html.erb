<%= render "shared/header" %>
 
<div class="main">
  <%= link_to '予定を追加する', new_event_path %>

  <%= month_calendar events: @events do |date, events| %>
  <%= date.day %>

    <% events.each do |event| %>
      <div>
        <%= link_to event.title, event %>
      </div>
    <% end %>
  <% end %>
</div>

<%= render "shared/footer" %>